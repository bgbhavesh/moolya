    "react-custom-scrollbars": "^4.2.1",
    "react-d3-basic": "1.6.11",
    "react-d3-tooltip": "1.4.14",
    "react-datetime": "^2.10.1",
    "react-dnd": "2.4.0",
    "react-dnd-html5-backend": "2.4.1",
    "react-dnd-touch-backend": "^0.3.12",
    "react-dom": "^15.4.2",
    "react-fontawesome": "^1.5.0",
    "react-komposer": "1.13.1",
    "react-meta-tags": "0.1.3",
    "react-motion": "0.4.7",
    "react-mounter": "1.2.0",
    "react-places-autocomplete": "3.0.0",
    "react-pure-render": "1.0.2",
    "react-rangeslider": "^2.0.1",
    "react-rating": "0.7.1",
    "react-redux": "5.0.6",
    "react-responsive-tabs": "^1.1.3",
    "react-scrollbar": "0.5.1",
    "react-scrollbar-js": "1.0.1",
    "react-select": "1.0.0-rc.3",
    "react-tab-panel": "2.2.7",
    "react-toastify": "1.7.0",
    "react-toastr": "2.8.2",
    "react-transition-group": "^1.1.3",
    "react-utils": "1.0.0",
    "reactstrap": "^4.5.0",
    "recompose": "^0.23.1",

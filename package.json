{
  "name": "application-name",
  "version": "1.0.0",
  "description": "Application description.",
  "scripts": {
    "start": "meteor --settings settings-dev.json",
    "test": "meteor --settings settings-dev.json test --driver-package practicalmeteor:mocha --port 5000",
    "lint": "eslint --ext js --ext jsx server || exit 0",
    "lint1": "eslint --ext js --ext jsx client || exit 0",
    "chimp-watch": "chimp --ddp=http://localhost:3000 --watch --mocha --path=tests",
    "chimp-test": "chimp --ddp=http://localhost:3000 --mocha --path=tests",
    "staging": "meteor deploy staging.meteor.com --settings settings-dev.json",
    "production": "meteor deploy production.meteor.com --settings settings-prod.json"
  },
  "devDependencies": {
    "babel-preset-es2015": "^6.24.1",
    "chimp": "^0.41.2",
    "eslint": "^3.8.1",
    "eslint-config-airbnb": "^12.0.0",
    "eslint-plugin-import": "^1.16.0",
    "eslint-plugin-jsx-a11y": "^2.2.3",
    "eslint-plugin-meteor": "^4.0.1",
    "eslint-plugin-react": "^6.4.1"
  },
  "eslintConfig": {
    "parserOptions": {
      "ecmaFeatures": {
        "jsx": true
      }
    },
    "plugins": [
      "meteor",
      "react"
    ],
    "extends": [
      "airbnb/base",
      "plugin:meteor/guide",
      "plugin:react/recommended"
    ],
    "env": {
      "browser": true
    },
    "globals": {
      "server": false,
      "browser": false,
      "expect": false
    },
    "rules": {
      "import/no-unresolved": 0,
      "import/imports-first": 0,
      "import/newline-after-import": 0,
      "import/no-dynamic-require": 0,
      "import/no-extraneous-dependencies": 0,
      "import/extensions": 0,
      "import/no-named-as-default": 0,
      "import/prefer-default-export": 0,
      "indent": [
        2,
        2,
        {
          "SwitchCase": 1
        }
      ],
      "jsx-a11y/aria-props": 2,
      "jsx-a11y/heading-has-content": 0,
      "jsx-a11y/href-no-hash": 2,
      "jsx-a11y/label-has-for": 2,
      "jsx-a11y/mouse-events-have-key-events": 2,
      "jsx-a11y/role-has-required-aria-props": 2,
      "jsx-a11y/role-supports-aria-props": 2,
      "max-len": 0,
      "newline-per-chained-call": 0,
      "no-confusing-arrow": 0,
      "no-console": 1,
      "no-use-before-define": 0,
      "prefer-template": 2,
      "class-methods-use-this": 0,
      "react/forbid-prop-types": 0,
      "react/jsx-first-prop-new-line": [
        2,
        "multiline"
      ],
      "react/jsx-filename-extension": 0,
      "react/jsx-no-target-blank": 0,
      "react/require-extension": 0,
      "react/self-closing-comp": 0,
      "require-yield": 0,
      "import/no-webpack-loader-syntax": 0,
      "no-param-reassign": 0,
      "no-underscore-dangle": [
        "error",
        {
          "allow": [
            "_id",
            "_ensureIndex",
            "_verifyEmailToken",
            "_resetPasswordToken",
            "_name"
          ]
        }
      ],
      "comma-dangle": [
        "error",
        {
          "arrays": "never",
          "objects": "never",
          "imports": "never",
          "exports": "never",
          "functions": "ignore"
        }
      ],
      "semi":0
    }
  },
  "dependencies": {
    "apollo-client": "^0.8.0",
    "apollo-server": "0.2.8",
    "async": "^2.3.0",
    "awesome-phonenumber": "2.1.0",
    "aws-config": "1.1.3",
    "aws-sdk": "^2.28.0",
    "babel-runtime": "^6.23.0",
    "base-64": "0.1.0",
    "base64-min": "2.0.0",
    "bcrypt": "0.8.7",
    "body-parser": "^1.15.2",
    "bootstrap": "3.3.7",
    "bufferutil": "3.0.2",
    "connect-multiparty": "2.0.0",
    "cors": "^2.8.1",
    "crypto-js": "^3.1.9-1",
    "csrf": "3.0.6",
    "custom-error": "0.2.1",
    "deep-diff": "^0.3.4",
    "deep-extend": "^0.4.1",
    "dinstar-sms-api": "0.1.5",
    "express": "^4.14.0",
    "extendify": "1.0.0",
    "fastclick": "^1.0.6",
    "geocoder": "^0.2.2",
    "google-map-react": "0.22.3",
    "gql-merge": "0.0.4",
    "graphql": "0.8.2",
    "graphql-parser": "2.1.0",
    "graphql-server-express": "0.5.2",
    "graphql-tag": "^0.1.13",
    "graphql-tools": "^0.9.0",
    "graphql-type-json": "0.1.4",
    "graphql-typings": "0.0.1-beta-2",
    "graphql-utilities": "0.0.14",
    "helmet": "3.6.1",
    "http-proxy": "1.16.2",
    "http-proxy-middleware": "^0.17.3",
    "immutability-helper": "^2.3.1",
    "jquery": "3.2.1",
    "jquery-mousewheel": "3.1.13",
    "libphonenumber-js": "^0.4.29",
    "loadash": "0.0.1",
    "lodash": "4.17.4",
    "malihu-custom-scrollbar-plugin": "3.1.5",
    "meteor-node-stubs": "^0.2.5",
    "minio": "^3.2.1",
    "mocha": "3.4.2",
    "moment": "^2.18.1",
    "omit-deep-lodash": "1.0.0",
    "points-cluster": "0.1.4",
    "prop-types": "15.5.10",
    "pug": "2.0.0-rc.3",
    "q": "^1.4.1",
    "querystring": "^0.2.0",
    "random-string": "^0.2.0",
    "rc-tabs": "^8.1.1",
    "react": "^15.5.4",
    "react-addons-css-transition-group": "^15.5.2",
    "react-addons-pure-render-mixin": "^15.5.2",
    "react-addons-transition-group": "^15.5.2",
    "react-apollo": "0.8.3",
    "react-big-calendar": "^0.14.4",
    "react-bootstrap": "0.31.2",
